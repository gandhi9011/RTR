<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="300dp"
    android:layout_margin="4dp"
    android:layout_marginBottom="4dp"
    android:layout_marginEnd="4dp"
    android:layout_marginLeft="4dp"
    android:layout_marginRight="4dp"
    android:layout_marginStart="4dp"
    android:layout_marginTop="4dp"
    android:layout_weight="2"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">



    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="Agent ID:" />

        <TextView
            android:id="@+id/agent_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="Inbound Calls:" />

        <TextView
            android:id="@+id/agent_inbound_calls"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="Answers Calls:" />

        <TextView
            android:id="@+id/agent_answers_call"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="Abandoned Calls:" />

        <TextView
            android:id="@+id/agent_abandoned_calls"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="Outbound Total Calls:" />

        <TextView
            android:id="@+id/agent_outbound_total_calls"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="Outbound Call Duration:" />

        <TextView
            android:id="@+id/agent_outbound_call_duration"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="ACD State:" />

        <TextView
            android:id="@+id/agent_acd_state"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="ACD State Duration :" />

        <TextView
            android:id="@+id/agent_acd_state_duration"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="Total Call Duration:" />

        <TextView
            android:id="@+id/agent_total_call_duration"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="Outbound Internal Calls:" />

        <TextView
            android:id="@+id/agent_outbound_internal_calls"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>

    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="Outbound External Calls:" />

        <TextView
            android:id="@+id/agent_outbound_external_calls"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="Inbound Outbound Toatal Calls:" />

        <TextView
            android:id="@+id/agent_inbound_outbound_total_calls"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="Inbound Outbound Toatal Calls Duration:" />

        <TextView
            android:id="@+id/agent_inbound_outbound_total_duration"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="Transfered Calls:" />

        <TextView
            android:id="@+id/agent_transfered_call"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|left"
            android:text="Voice Mail:" />

        <TextView
            android:id="@+id/agent_voice_mail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="right|center_horizontal"
            android:paddingLeft="?android:attr/expandableListPreferredChildPaddingLeft"/>
    </LinearLayout>



</LinearLayout>
